:M001   rdf:type  :Marriage , owl:NamedIndividual .

:MarriedTo  rdf:type  owl:SymmetricProperty , <https://hrssilva.github.io/ontology/omice.ttl#RelationType> , owl:ObjectProperty , owl:NamedIndividual ;
        rdfs:domain  :Person ;
        rdfs:range   :Person .

:Marriage  rdf:type      <https://hrssilva.github.io/ontology/omice.ttl#EventType> , owl:NamedIndividual , owl:Class ;
        rdfs:subClassOf  <https://hrssilva.github.io/ontology/omice.ttl#RelationModificationEvent> ;
        <https://hrssilva.github.io/ontology/omice.ttl#ConcernsModificationAsObject>
                :Person ;
        <https://hrssilva.github.io/ontology/omice.ttl#ConcernsModificationAsSubject>
                :Person ;
        <https://hrssilva.github.io/ontology/omice.ttl#GivesRelation>
                :MarriedTo .

:Person  rdf:type        <https://hrssilva.github.io/ontology/omice.ttl#ObjectType> , owl:Class , owl:NamedIndividual ;
        rdfs:subClassOf  :Object .

:P001   rdf:type    :Person , owl:NamedIndividual ;
        :MarriedTo  :P002 ;
        <https://hrssilva.github.io/ontology/omice.ttl#SubjectModifiedBy>
                :M001 .

