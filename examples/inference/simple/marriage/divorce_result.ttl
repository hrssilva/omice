@prefix :        <http://www.example.org/marriage.ttl/> .
@prefix dash:    <http://datashapes.org/dash#> .
@prefix graphql: <http://datashapes.org/graphql#> .
@prefix omice:   <https://hrssilva.github.io/ontology/omice.ttl#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:      <http://www.w3.org/ns/shacl#> .
@prefix swa:     <http://topbraid.org/swa#> .
@prefix tosh:    <http://topbraid.org/tosh#> .
@prefix xml:     <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

:Divorce  rdf:type             owl:NamedIndividual , omice:EventType , owl:Class;
        rdfs:subClassOf        omice:RelationModificationEvent;
        omice:ConcernsModificationAsObject
                :Person;
        omice:ConcernsModificationAsSubject
                :Person;
        omice:RemovesRelation  :MarriedTo .

:Object  rdf:type        omice:ObjectType , owl:Class , owl:NamedIndividual;
        rdfs:subClassOf  omice:TypeOfParticulars .

:D001   rdf:type  owl:NamedIndividual , :Divorce .

:P002   rdf:type                :Person , owl:NamedIndividual;
        omice:ObjectModifiedBy  :D001 .

<http://www.example.org/marriage.ttl>
        rdf:type     owl:Ontology;
        owl:imports  <https://hrssilva.github.io/ontology/omice.ttl> .

:P001   rdf:type                 :Person , owl:NamedIndividual;
        omice:SubjectModifiedBy  :D001 .

:Person  rdf:type        omice:ObjectType , owl:Class , owl:NamedIndividual;
        rdfs:subClassOf  :Object .

:Marriage  rdf:type          omice:EventType , owl:NamedIndividual , owl:Class;
        rdfs:subClassOf      omice:RelationModificationEvent;
        omice:ConcernsModificationAsObject
                :Person;
        omice:ConcernsModificationAsSubject
                :Person;
        omice:GivesRelation  :MarriedTo .

:MarriedTo  rdf:type  owl:SymmetricProperty , omice:RelationType , owl:ObjectProperty , owl:NamedIndividual;
        rdfs:domain  :Person;
        rdfs:range   :Person .
